{
  "name": "codebusters-ciphers",
  "version": "0.0.4",
  "description": "ciphers for science olympiad's codebusters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"__tests__/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "preversion": "npm run lint && npm run test",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/swarajd/codebusters-ciphers.git"
  },
  "keywords": [
    "codebusters",
    "cryptography",
    "ciphers",
    "science",
    "olympiad"
  ],
  "author": "Swaraj Dhumne <swaraj.dhumne@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/swarajd/codebusters-ciphers/issues"
  },
  "homepage": "https://github.com/swarajd/codebusters-ciphers#readme",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "husky": "^1.3.1",
    "jest": "^23.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}
